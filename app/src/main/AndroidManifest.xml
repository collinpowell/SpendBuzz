<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.collinpowell.spendbuzz">

    <application
        android:allowBackup="true"
        android:icon="@drawable/spend_buzz"
        android:label="@string/app_name"
        android:roundIcon="@drawable/spend_buzz"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Actitvities.CreateNewBudgetForm"></activity>
        <activity android:name=".Actitvities.ListOfLists" />
        <activity android:name=".Actitvities.MarketList" />
        <activity android:name=".Actitvities.Budget" />
        <activity android:name=".Actitvities.CreateNewBudget" />
        <activity android:name=".Actitvities.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Actitvities.IntroScreen" />
        <activity android:name=".Actitvities.MainActivity" />
    </application>

</manifest>